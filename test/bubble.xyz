    whilst (i < (n - 1)) {
		j := i + 1;
        whilst (j < (n - 2)) {
            when (arr[j] > arr[(j + 1)]) {
                temp := arr[i];
                arr[i] := arr[j];
                arr[j] := temp;
            }
            j := j + 1;
        }
        i := i + 1;
    }

