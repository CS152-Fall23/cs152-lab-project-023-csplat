void bubbleSort ?int arr[], int n?
{
    int i, j, temp := 0
    
    whilst (i < n - 1) {
		j = i + 1  
        whilst (j < n - 1) {
            when (arr[j] > arr[j + 1]) {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            }
            ++j;
        }
        ++i;
    }
}
